class DoWhile										//1
{																
	public static void main (String[] args)							//2
	{						
		int num=100;										//3
		do																		
		{
			System.out.println("DoWhile Countup:"+num);					//4
		}
		while (num< 1000) ;
		num +=10 ;                                  									//5
	}
}
